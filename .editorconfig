# editorconfig.org
root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = tab
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.yaml]
indent_style = space
indent_size = 2

[*.yml]
indent_style = space
indent_size = 2

[*.tf]
indent_style = space
indent_size = 2
[package.json]
indent_style = space
indent_size = 2
[package-lock.json]
indent_style = space
indent_size = 2
