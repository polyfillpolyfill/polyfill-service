{
  "name": "polyfill-service",
  "description": "A polyfill combinator",
  "private": true,
  "contributors": [
    {
      "name": "FT Origami",
      "email": "origami.support@ft.com",
      "url": "https://origami.ft.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/financial-times/polyfill-service.git"
  },
  "bugs": {
    "url": "https://github.com/financial-times/polyfill-service/issues"
  },
  "scripts": {
    "toppops": "bash .github/workflows/toppops.sh",
    "fmt": "terraform fmt fastly/terraform/ && eslint . --fix",
    "lint": "eslint .",
    "start": "./start_server.sh server/index.js",
    "test": "run-s test:*",
    "test:unit": "nyc --reporter=lcov --reporter=text mocha test/unit/**/*.test.js --recursive",
    "test:integration": "mocha test/integration/*.test.js test/integration/**/*.test.js --recursive --bail --delay --check-leaks --forbid-only --forbid-pending",
    "clean": "rimraf ./dist ./.cache ./.cache_eleventy",
    "build": "run-s production",
    "build:dev": "run-s production:*",
    "dev": "npm run clean && nodemon --exec \"time npm run build:dev && npm run start\"",
    "production": "run-s clean production:*",
    "production:eleventy": "eleventy",
    "production:parcel": "mkdir -p './dist/v3/json' && cp -R './.cache_eleventy/json/' './dist/v3/' && parcel build ./.cache_eleventy/**/*.html ./.cache_eleventy/*.html --out-dir ./dist/v3 --public-url /v3",
    "postshrinkwrap": "lockspot flat --pattern '^@financial-times/o-' --file './package-lock.json' || (echo -e \"\\033[31mChecking the package-lock.json file showed some FT components were included multiple times with different versions. This can result in client builds including multiple copies of the CSS/JS for those packages, increasing build size. Please check the dependency ranges to ensure each dependency is only included once.\n\nFT components included more than once, with their count:\\033[0m\"; lockspot depcount --min 2 --pattern '^@financial-times/' --file './package-lock.json' && exit 1)",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "heroku-run-build-script": true,
  "license": "MIT",
  "engines": {
    "node": "11 - 12",
    "npm": ">=6.9.0"
  },
  "dependencies": {
    "@financial-times/origami-service": "^4.1.2",
